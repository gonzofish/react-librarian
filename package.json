{
  "name": "react-librarian",
  "version": "0.0.0",
  "description": "A React library scaffolding tool",
  "main": "index.js",
  "scripts": {
    "test": "tap \"test/**/*.spec.js\"",
    "test:cov": "tap \"test/**/*.spec.js\" --coverage",
    "reportCoverage": "tap --coverage-report=lcov && codecov",
    "tagVersion": "np",
    "watch": "nodemon -q -x \"npm run test:cov\""
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gonzofish/react-librarian.git"
  },
  "keywords": [
    "react",
    "scaffold",
    "scaffolding",
    "typescript",
    "webpack",
    "rollup"
  ],
  "author": "Matt Fehskens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gonzofish/react-librarian/issues"
  },
  "homepage": "https://github.com/gonzofish/react-librarian#readme",
  "dependencies": {
    "chalk": "^2.1.0",
    "erector-set": "^1.0.0-beta.3",
    "fs-extra": "^4.0.1"
  },
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-plugin-node": "^5.1.1",
    "nodemon": "^1.11.0",
    "np": "^2.16.0",
    "sinon": "^3.2.1",
    "tap": "^10.7.2"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
